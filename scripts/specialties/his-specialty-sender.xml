          <script>// Verifyt if message is from nestjs parser
var rawMsg = msg;
var hasHash = rawMsg.indexOf(&apos;#&apos;);
if( hasHash !== -1 &amp;&amp; hasHash &lt; 6 ){
	rawMsg = rawMsg.substring(hasHash + 1);
};

var parsedMessage = JSON.parse(rawMsg);
var xmlMessage = parsedMessage.data;
var pattern = parsedMessage.pattern;

//Con esto tenemos ya para manejar los patrones de TCP/IP, es importante que los datos vengan en un mensaje en json con: {&quot;pattern&quot;: &quot;patron&quot;, &quot;data&quot;: &quot;XML&quot;}

if(pattern === &quot;nueva-especialidad&quot;){
//	&lt;result&gt;&lt;codigo&gt;EJEMPLO&lt;/codigo&gt;&lt;descripcion&gt;Pediatria&lt;/descripcion&gt;&lt;/result&gt;
	var parsedJson = XmlUtil.toJson(xmlMessage);
	var parsedResult = JSON.parse(parsedJson)[&apos;result&apos;];
	
	channelMap.put(&apos;info&apos;, JSON.stringify({
			&quot;code&quot;: parsedResult[&quot;codigo&quot;],
			&quot;name&quot;: parsedResult[&quot;descripcion&quot;]
		}));

// Configuramos que destinatario enviara el mensaje, recibe id o nombre y puede ser array
	destinationSet.removeAllExcept([&apos;CrearEspecialidad&apos;]);
} else if (pattern === &quot;editar-especialidad&quot;) {

	var parsedJson = XmlUtil.toJson(xmlMessage);
	var parsedResult = JSON.parse(parsedJson)[&apos;result&apos;];
	
	channelMap.put(&apos;info&apos;, JSON.stringify({
		&quot;code&quot;: parsedResult[&quot;codigo&quot;],
		&quot;name&quot;: parsedResult[&quot;descripcion&quot;]
	}));

// Configuramos que destinatario enviara el mensaje, recibe id o nombre y puede ser array
	destinationSet.removeAllExcept([&apos;EditarEspecialidad&apos;]);
} else if (pattern === &quot;eliminar-especialidad&quot;) {

	var parsedJson = XmlUtil.toJson(xmlMessage);
	var parsedResult = JSON.parse(parsedJson)[&apos;result&apos;];
	
	channelMap.put(&apos;info&apos;, JSON.stringify({
		&quot;code&quot;: parsedResult[&quot;codigo&quot;],
	}));

// Configuramos que destinatario enviara el mensaje, recibe id o nombre y puede ser array
	destinationSet.removeAllExcept([&apos;EliminarEspecialidad&apos;]);
}

//Imprimir el mensaje para verificar que todo sea correcto
// logger.info(&quot;Mensaje XMLm extraido&quot; + xmlMessage);</script>
